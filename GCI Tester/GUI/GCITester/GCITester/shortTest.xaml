        mc:Ignorable="d"
        Title="shortTest" Height="772.427" Width="537.088"
        >
    <Grid>
        <ComboBox x:Name="partNumberBox" HorizontalAlignment="Left" Margin="300,46,0,0" VerticalAlignment="Top" Width="185" Height="40" BorderThickness="5" BorderBrush="#FF15315F"/>
