        <Button x:Name="button_Copy1" Content="Part Editor" HorizontalAlignment="Left" Margin="10,233,0,0" VerticalAlignment="Top" Width="222" Height="47" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="20" FontWeight="Bold" FontFamily="Microsoft JhengHei UI"/>
        <Button x:Name="button_Copy2" Content="Test Editor" HorizontalAlignment="Left" Margin="10,300,0,0" VerticalAlignment="Top" Width="222" Height="49" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="20" FontWeight="Bold" FontFamily="Microsoft JhengHei UI"/>
        <Button x:Name="button_Copy3" Content="Edit Production Limits" HorizontalAlignment="Left" Margin="262,99,0,0" VerticalAlignment="Top" Width="222" Height="48" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="20" FontWeight="Bold" FontFamily="Microsoft JhengHei UI"/>
        <Button x:Name="button_Copy4" Content="Production Test" HorizontalAlignment="Left" Margin="262,166,0,0" VerticalAlignment="Top" Width="222" Height="48" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="20" FontWeight="Bold" FontFamily="Microsoft JhengHei UI"/>
