        <Button x:Name="button_Copy6" Content="Lifetime Report" HorizontalAlignment="Left" Margin="262,300,0,0" VerticalAlignment="Top" Width="222" Height="49" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="20" FontWeight="Bold" FontFamily="Microsoft JhengHei UI"/>
        <Button x:Name="button_Copy7" Content="Settings" HorizontalAlignment="Left" Margin="496,99,0,0" VerticalAlignment="Top" Width="222" Height="48" Background="#FF15315F" Foreground="#FFFDFDFD" FontSize="20" FontWeight="Bold" FontFamily="Microsoft JhengHei UI" Grid.ColumnSpan="2" Click="button_Copy7_Click"/>

    </Grid>
